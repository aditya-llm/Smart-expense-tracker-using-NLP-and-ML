      "cell_type": "code",
      "source": [
        "# -----------------------------------------------------------\n",
        "# Cell 5: Main Execution\n",
        "# -----------------------------------------------------------\n",
        "\n",
        "# 3. Process each downloaded PDF\n",
        "pdf_files = sorted([f for f in os.listdir(DOWNLOAD_DIR) if f.endswith(\".pdf\")])\n",
        "for pdf_file in pdf_files:\n",
        "    english_pdf_path = os.path.join(DOWNLOAD_DIR, pdf_file)\n",
        "    hindi_pdf_path = os.path.join(OUTPUT_DIR, pdf_file.replace('.pdf', '_hindi.pdf'))\n",
        "    # NEW: Define the output path for the cleaned text file\n",
        "    cleaned_text_path = os.path.join(CLEANED_TEXT_DIR, pdf_file.replace('.pdf', '_cleaned.txt'))\n",
        "\n",
        "\n",
        "    print(f\"\\n{'='*20} PROCESSING: {pdf_file} {'='*20}\")\n",
        "\n",
        "    # This function is now the improved version from Step 2\n",
        "    structured_content = extract_content_with_layout(english_pdf_path)\n",
        "\n",
        "    # Gather text and references for translation\n",
        "    # This list will now contain full sentences/lines, not fragments\n",
        "    all_english_texts = [item[\"text\"] for page in structured_content for item in page[\"content\"] if item[\"type\"] == \"text\"]\n",
        "    text_references = [(p_idx, i_idx) for p_idx, page in enumerate(structured_content) for i_idx, item in enumerate(page[\"content\"]) if item[\"type\"] == \"text\"]\n",
        "\n",
        "    # --- NEW: Save the cleaned text to a file ---\n",
        "    print(f\"-> Saving {len(all_english_texts)} cleaned lines to {cleaned_text_path}\")\n",
        "    with open(cleaned_text_path, 'w', encoding='utf-8') as f:\n",
        "        for line in all_english_texts:\n",
        "            f.write(line + '\\n')\n",
        "    # --- End of new section ---\n",
        "\n",
        "    if all_english_texts:\n",
        "        print(f\"-> Translating {len(all_english_texts)} lines of text...\")\n",
        "        translated_texts = batch_translate(all_english_texts, src_lang, tgt_lang, model, tokenizer, ip)\n",
        "\n",
        "        # Put translated text back into the structure\n",
        "        for i, (page_idx, item_idx) in enumerate(text_references):\n",
        "            structured_content[page_idx][\"content\"][item_idx][\"text\"] = translated_texts[i]\n",
        "    else:\n",
        "        print(\"No text found in this PDF to translate.\")\n",
        "\n",
        "    # Create the final Hindi PDF\n",
        "    create_hindi_pdf(structured_content, hindi_pdf_path)\n",
        "    torch.cuda.empty_cache()\n",
        "\n",
        "print(f\"\\n🎉 All PDF files have been processed and translated!\")"
      ],
