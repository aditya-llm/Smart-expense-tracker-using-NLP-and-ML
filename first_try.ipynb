      "cell_type": "markdown",
      "source": [
        "## **All helper functions**"
      ],
      "metadata": {
        "id": "gXxEqKQ5Gc7R"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# -----------------------------------------------------------\n",
        "# Cell 4: All Function Definitions\n",
        "# -----------------------------------------------------------\n",
        "\n",
        "def initialize_model_and_tokenizer(ckpt_dir, quantization):\n",
        "    if quantization == \"4-bit\":\n",
        "        qconfig = BitsAndBytesConfig(\n",
        "            load_in_4bit=True,\n",
        "            bnb_4bit_use_double_quant=True,\n",
        "            bnb_4bit_compute_dtype=torch.bfloat16,\n",
        "        )\n",
        "    elif quantization == \"8-bit\":\n",
        "        qconfig = BitsAndBytesConfig(\n",
        "            load_in_8bit=True,\n",
        "            bnb_8bit_use_double_quant=True,\n",
        "            bnb_8bit_compute_dtype=torch.bfloat16,\n",
        "        )\n",
        "    else:\n",
        "        qconfig = None\n",
        "\n",
        "    tokenizer = AutoTokenizer.from_pretrained(ckpt_dir, trust_remote_code=True)\n",
        "    model = AutoModelForSeq2SeqLM.from_pretrained(\n",
        "        ckpt_dir,\n",
        "        trust_remote_code=True,\n",
        "        low_cpu_mem_usage=True,\n",
        "        quantization_config=qconfig,\n",
        "    )\n",
        "\n",
        "    if qconfig == None:\n",
        "        model = model.to(DEVICE)\n",
        "        if DEVICE == \"cuda\":\n",
        "            model.half()\n",
        "\n",
        "    model.eval()\n",
        "\n",
        "    return tokenizer, model\n",
        "\n",
        "# with progress bar:\n",
        "def batch_translate(input_sentences, src_lang, tgt_lang, model, tokenizer, ip):\n",
        "    translations = []\n",
        "    # MODIFICATION: Wrap the range iterator with tqdm for a progress bar\n",
        "    for i in tqdm(range(0, len(input_sentences), BATCH_SIZE), desc=\"Translating Batches\"):\n",
        "        batch = input_sentences[i : i + BATCH_SIZE]\n",
        "\n",
        "        # Preprocess the batch and extract entity mappings\n",
        "        batch = ip.preprocess_batch(batch, src_lang=src_lang, tgt_lang=tgt_lang)\n",
        "\n",
        "        # Tokenize the batch and generate input encodings\n",
        "        inputs = tokenizer(\n",
        "            batch,\n",
        "            truncation=True,\n",
        "            padding=\"longest\",\n",
        "            return_tensors=\"pt\",\n",
        "            return_attention_mask=True,\n",
        "        ).to(DEVICE)\n",
        "\n",
        "        # Generate translations using the model\n",
        "        with torch.no_grad():\n",
        "            generated_tokens = model.generate(\n",
        "                **inputs,\n",
        "                use_cache=True,\n",
        "                min_length=0,\n",
        "                max_length=512, # make 500 for faster generation\n",
        "                num_beams=5,\n",
        "                num_return_sequences=1,\n",
        "            )\n",
        "\n",
        "        # Decode the generated tokens into text\n",
        "        generated_tokens = tokenizer.batch_decode(\n",
        "            generated_tokens,\n",
        "            skip_special_tokens=True,\n",
        "            clean_up_tokenization_spaces=True,\n",
        "        )\n",
        "\n",
        "        # Postprocess the translations, including entity replacement\n",
        "        translations += ip.postprocess_batch(generated_tokens, lang=tgt_lang)\n",
        "\n",
        "        del inputs\n",
        "        torch.cuda.empty_cache()\n",
        "\n",
        "    return translations\n",
        "\n",
        "# -----------------------------------------------------------\n",
        "# Function for downloading PDF's\n",
        "# -----------------------------------------------------------\n",
        "\n",
        "import time # Make sure this is imported in Cell 2\n",
        "\n",
        "def download_pdfs(urls, save_dir):\n",
        "    \"\"\"\n",
        "    Downloads PDFs with a browser-like User-Agent and a retry mechanism.\n",
        "    \"\"\"\n",
        "    print(\"--- Downloading PDFs (with retry logic) ---\")\n",
        "    headers = {\n",
        "        'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36'\n",
        "    }\n",
        "    for code in tqdm(urls, desc=\"Downloading PDFs\"):\n",
        "        url = BASE_URL.format(code)\n",
        "        filename = os.path.join(save_dir, f\"{code}.pdf\")\n",
        "        if os.path.exists(filename):\n",
        "            continue\n",
        "        for attempt in range(3):\n",
        "            try:\n",
        "                response = requests.get(url, headers=headers, stream=True, timeout=30)\n",
        "                response.raise_for_status()\n",
        "                with open(filename, \"wb\") as f:\n",
        "                    for chunk in response.iter_content(chunk_size=8192):\n",
        "                        f.write(chunk)\n",
        "                break\n",
        "            except requests.exceptions.RequestException as e:\n",
        "                print(f\"Error downloading {url} on attempt {attempt + 1}: {e}\")\n",
        "                if attempt < 2:\n",
        "                    print(\"Retrying in 5 seconds...\")\n",
        "                    time.sleep(5)\n",
        "                else:\n",
        "                    print(f\"Failed to download {url} after 3 attempts.\")\n",
        "\n",
        "# (In Cell 4 - Replace the old function with this new one)\n",
        "import re # Make sure 'import re' is in Cell 2\n",
        "\n",
        "def clean_text(text):\n",
        "    \"\"\"\n",
        "    Cleans extracted text with advanced rules to remove artifacts,\n",
        "    repetitive noise, and junk characters.\n",
        "    \"\"\"\n",
        "    # 1. Remove common NCERT headers/footers\n",
        "    text = re.sub(r'Rationalised\\s+2023-24', '', text)\n",
        "\n",
        "    # 2. NEW: Remove meaningless repeating character patterns like \"अ. अ. अ.\"\n",
        "    # This regex finds a character, then checks if it's repeated with \". \"\n",
        "    # at least twice. The '\\1' is a backreference to the captured character.\n",
        "    text = re.sub(r'(\\w)(\\.\\s\\1){2,}', '', text)\n",
        "\n",
        "    # 3. Remove strings of purely repetitive, non-alphanumeric characters (like '....' or '////')\n",
        "    # This looks for 3 or more repeating non-word, non-space characters.\n",
        "    text = re.sub(r'([^\\w\\s])\\1{2,}', '', text)\n",
        "\n",
        "    # 4. Remove standalone page numbers or stray numbers.\n",
        "    # We will only keep \"words\" that contain at least one letter.\n",
        "    # This effectively removes page numbers like \"25\" but keeps \"Chapter 1\".\n",
        "    words = text.split()\n",
        "    # A list comprehension to filter the words\n",
        "    cleaned_words = [word for word in words if any(c.isalpha() for c in word)]\n",
        "    text = ' '.join(cleaned_words)\n",
        "\n",
        "    # 5. Normalize all whitespace to single spaces\n",
        "    text = re.sub(r'\\s+', ' ', text).strip()\n",
        "\n",
        "    return text\n",
        "\n",
        "# -----------------------------------------------------------\n",
        "# Function for Content extraction along with formatting\n",
        "# -----------------------------------------------------------\n",
        "\n",
        "def extract_content_with_layout(pdf_path):\n",
        "    \"\"\"\n",
        "    Extracts content (text and images) from a PDF, intelligently merging\n",
        "    text spans into coherent lines to provide better context for translation.\n",
        "    \"\"\"\n",
        "    print(f\"\\n--- Extracting content from {os.path.basename(pdf_path)} ---\")\n",
        "    doc = fitz.open(pdf_path)\n",
        "    all_pages_content = []\n",
        "\n",
        "    for page in tqdm(doc, desc=\"Extracting pages\"):\n",
        "        page_content = []\n",
        "        page_height, page_width = page.rect.height, page.rect.width\n",
        "\n",
        "        # 1. Extract Images\n",
        "        for img in page.get_images(full=True):\n",
        "            xref = img[0]\n",
        "            base_image = doc.extract_image(xref)\n",
        "            img_rects = page.get_image_rects(img)\n",
        "            if img_rects:\n",
        "                page_content.append({\n",
        "                    \"type\": \"image\",\n",
        "                    \"bbox\": img_rects[0], # Use the first rect for the image\n",
        "                    \"bytes\": base_image[\"image\"]\n",
        "                })\n",
        "\n",
        "        # 2. Extract Text, but merge spans into lines\n",
        "        blocks = page.get_text(\"dict\", flags=11)[\"blocks\"]\n",
        "        for block in blocks:\n",
        "            if block['type'] == 0:  # This is a text block\n",
        "                for line in block[\"lines\"]:\n",
        "                    # Join all span texts in a line, clean it\n",
        "                    line_text = \" \".join([clean_text(span[\"text\"]) for span in line[\"spans\"]]).strip()\n",
        "                    if line_text:\n",
        "                        # Use the line's bounding box and the first span's font size\n",
        "                        page_content.append({\n",
        "                            \"type\": \"text\",\n",
        "                            \"bbox\": fitz.Rect(line[\"bbox\"]),\n",
        "                            \"text\": line_text,\n",
        "                            \"font_size\": line[\"spans\"][0][\"size\"]\n",
        "                        })\n",
        "\n",
        "        # 3. Sort all content by vertical position for correct ordering\n",
        "        page_content.sort(key=lambda item: item[\"bbox\"].y0)\n",
        "\n",
        "        all_pages_content.append({\n",
        "            \"height\": page_height,\n",
        "            \"width\": page_width,\n",
        "            \"content\": page_content\n",
        "        })\n",
        "\n",
        "    doc.close()\n",
        "    return all_pages_content\n",
        "\n",
        "# -----------------------------------------------------------\n",
        "# Function for Creating Hindi PDF's\n",
        "# -----------------------------------------------------------\n",
        "\n",
        "def create_hindi_pdf(structured_content, output_path):\n",
        "    \"\"\"\n",
        "    Creates a new PDF, now with proper handling for transparent images\n",
        "    to avoid black backgrounds.\n",
        "    \"\"\"\n",
        "    print(f\"\\n--- Creating Hindi PDF at {output_path} ---\")\n",
        "\n",
        "    pdfmetrics.registerFont(TTFont(HINDI_FONT_NAME, HINDI_FONT_PATH))\n",
        "\n",
        "    if not structured_content:\n",
        "        print(\"Cannot create PDF, no content found.\")\n",
        "        return\n",
        "\n",
        "    first_page_dims = (structured_content[0][\"width\"], structured_content[0][\"height\"])\n",
        "    c = canvas.Canvas(output_path, pagesize=first_page_dims)\n",
        "\n",
        "    for page_data in tqdm(structured_content, desc=\"Building PDF pages\"):\n",
        "        page_width, page_height = page_data[\"width\"], page_data[\"height\"]\n",
        "        c.setPageSize((page_width, page_height))\n",
        "\n",
        "        for item in page_data[\"content\"]:\n",
        "            bbox = item[\"bbox\"]\n",
        "            x0, y0 = bbox.x0, page_height - bbox.y1\n",
        "            width, height = bbox.width, bbox.height\n",
        "\n",
        "            if item[\"type\"] == \"text\":\n",
        "                c.setFont(HINDI_FONT_NAME, item[\"font_size\"])\n",
        "                c.drawString(x0, y0 + height - item[\"font_size\"], item[\"text\"])\n",
        "\n",
        "            elif item[\"type\"] == \"image\":\n",
        "                try:\n",
        "                    img_data = io.BytesIO(item[\"bytes\"])\n",
        "                    img_reader = ImageReader(img_data)\n",
        "\n",
        "                    # --- THE FIX IS HERE ---\n",
        "                    # Add the mask='auto' parameter to handle transparency correctly.\n",
        "                    c.drawImage(img_reader, x0, y0, width=width, height=height,\n",
        "                                preserveAspectRatio=True, anchor='nw', mask='auto')\n",
        "\n",
        "                except Exception as e:\n",
        "                    # Add more detail to the error message for better debugging\n",
        "                    print(f\"Warning: Could not draw image. BBox: {bbox}. Error: {e}\")\n",
        "\n",
        "        c.showPage()\n",
        "\n",
        "    c.save()\n",
        "    print(\"✅ Hindi PDF created successfully!\")\n",
        "\n",
        "print(\"✅ All helper functions are defined.\")"
      ],
